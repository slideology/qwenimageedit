{
  "mcpServers": {
    "browser": {
      "command": "npx",
      "args": ["@browsermcp/mcp", "--transport", "stdio"],
      "env": {
        "NODE_ENV": "development"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--transport", "stdio"],
      "env": {
        "NODE_ENV": "development"
      }
    }
  }
}